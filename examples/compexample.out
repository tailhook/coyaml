SimpleHTTPServer:
  log-level: 3
  log-file: /var/log/test.log
  listen:
    host: localhost
    port: 80
    unix-socket: 
    fd: 0
  max-request-size: 1048576
  directory-indexes:
  - index
  - index.html
  - index.php
  root: /var/www
  server-string: coyaml-sampleserver/$coyaml_version
  extra-headers:
    X-Test: OK
    X-Fortune: 18+
    X-Test2: OK
    X-Anchor: _var_
    X-Var: _var_
    X-Subst: hello_var_
    X-Subst2: hello_var_world
    X-No-Var: hello
    X-Uservar: hello example
    X-Version: 0.3.3
    X-Integer: 123 bytes
    X-Cli: value from CLI
  http-forward:
  - host: 192.168.0.1
    port: 80
    unix-socket: 
  - host: 192.168.0.2
    port: 80
    unix-socket: 
  - host: 192.168.0.3
    port: 8080
    unix-socket: 
  - host: 192.168.0.5
    port: 9980
    unix-socket: 
  - host: 192.168.0.9
    port: 80
    unix-socket: 
  - host: 
    port: 80
    unix-socket: /var/run/internal_http
  status-socket:
    value: tcp://127.0.0.1:1234
  zmq-forward:
    value:
    - value: tcp://127.0.0.1:123
  intvalue:
    value: 2
  intvalue2:
    value: 123
  responses:
    default:
      code: 200
      status: OK
      headers:
        Content-Type: text/html
        X-Fortune: no
      body: "<!DOCTYPE html>\n<html>\n    <head><title>Hello</title></head>\n    <body>\n
        \       <h1>Hello</h2>\n        This is an empty site, actually!\n    </body>\n</html>\n"
    not-found:
      code: 500
      status: Error
      headers:
        Content-Type: text/html
      body: Error
    internal-error:
      code: 500
      status: Error
      headers:
        Content-Type: text/html
      body: Error
